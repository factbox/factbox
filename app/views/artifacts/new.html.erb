<%= render 'projects/basic_info' %>

<div class="d-flex flex-column">
  <div class="row">
    <div class="col-md-12">
      <%= render 'projects/home_tabs' %>

      <div class="mt-3 pl-3">
        <h5 class="text-secondary">Choose the artifact that you want create</h5>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-8">
      <div class="page-content">
        <ul class="artifact-types">
          <% @artifacts.each do |artifact| %>
            <li>
              <a href="/projects/<%= @project.name %>/artifacts/new/<%= artifact.name %>">
                <%= artifact.name %>
              </a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
