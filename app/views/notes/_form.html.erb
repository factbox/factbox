<%= simple_form_for(@artifact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description, as: :text %>
    <%= f.input :content, as: :text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-success pull-right' %>
  </div>
<% end %>
