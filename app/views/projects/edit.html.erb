<div class="row">
  <div class="col-md-12">
    <div class="page-head">
      <h1>Edit project</h1>
      <small>Settings of project</small>
    </div>
  </div>
</div>

<div class="row mt-3">
  <div class="col-md-6">
    <%= render 'form', note: @project %>
  </div>
  <div class="col-md-3 offset-md-2">
    <%= simple_form_for(@project) do |f| %>
      <%= render 'logo' %>
      <%= f.input :logo, label: 'Project picture', class: "mt-3" %>
      <%= f.button :submit, value: 'Update picture', class: 'btn btn-success'%>
    <% end %>
  </div>
</div>
